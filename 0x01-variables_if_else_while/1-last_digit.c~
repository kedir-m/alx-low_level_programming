#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/**
 * main - Entry point
 *
 * Return: 0 (success) Always
 */
int main(void)
{
int n;
int digit=n%10;
srand(time(0));
n = rand() - RAND_MAX / 2;
if (digit > 5)
{
printf("Last digit of %d is and is greater than 5\n", n);
}
else if (digit == 0)
{
printf("Last digit of %d is and is 0\n", n);
}
else
{
printf("%d is negative\n", n);
}
return (0);
}
