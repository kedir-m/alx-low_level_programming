#include <stdio.h>
#include "main.h"
/**
 * jack_bauer - last digit
 *
 *
 *
 */
void times_table(void)
{
	int i, j;

for (i = 0; i <= 9; i++)
{
	for(j = 0; j <= 9; j++)
	{
		printf("%d",i*j);
		if ((i <= 9 && j < 1) || (i <= 1 && j <= 9) || (i == 2 && j < 4) || (i == 3 && j < 3) || (i == 4 && j < 2))
		{
			_putchar(' ');
			_putchar(' ');
		}
		else
		{
			_putchar(' ');
		}
	}
	_putchar('\n');
}
}
