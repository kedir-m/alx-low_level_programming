#include <stdio.h>
#include "main.h"
/**
 * print_last_digit - last digit
 * @n: the number to be evaluated
 * Return: last digit
 *
 */
void jack_bauer(void)
{
	int i, j;

	for (i = 0; i < 24; i++)
	{
		for (j = 0; i < 60; j++)
		{
			_putchar((i / 10) + '0' );
			_putchar((i % 10) + '0' );
			_putchar(':');
			_putchar((j / 10) + '0' );
			_putchar((j % 10) + '0' );
		}
		_putchar(10);
	}
	return (0);
}
